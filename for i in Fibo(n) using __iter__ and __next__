class Fibo:
    def __init__(self,n):
        self.n=n
    def __iter__(self):
        self.a=0
        self.b=1
        return self
    def __next__(self):
        if(self.a <= self.n-1):
            result=self.a
            self.a,self.b=self.b,self.a+self.b
            return result
        else:
            raise StopIteration
        

n=int(input("Enter number for Fibonacci Series: "))
for i in Fibo(n):
    print(i)
