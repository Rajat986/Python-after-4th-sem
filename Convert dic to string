abc=input("Enter string: ")
def c_cs_to_dict():
    dicti={}
    for i in abc.split(';'):
        a,b=i.split('=')
        dicti[a]=b
    return dicti

def c_dict_to_cs(a):
    l=[]
    for i in a.items():
        l1=list(i)
        st='='.join(l1)
        l.append(st)
        string=';'.join(l)
    return string

a=c_cs_to_dict()
print(a)
stri=c_dict_to_cs(a)
print(stri)
